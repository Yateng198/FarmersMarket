<Window x:Class="FarmersMarketApp.Customer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FarmersMarketApp"
        mc:Ignorable="d"
        Title="Customer" Height="450" Width="800">
    <Grid>
        <Button Content="Home Page" HorizontalAlignment="Left" Margin="627,352,0,0" VerticalAlignment="Top" Height="42" Width="102" Click="Button_Click">
            <Button.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>

    </Grid>
</Window>
